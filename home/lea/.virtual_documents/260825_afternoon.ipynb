def count_vowels(word):
    vowels = "aeiou"
    count = 0
    for letter in word.lower(): 
        if letter in vowels:
            count += 1
    if count > 0:
        print(f"Number of vowels in {word!r} is {count}")
    else:
        print(f"No vowels in {word!r}.")
    return count

